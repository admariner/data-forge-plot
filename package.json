{
    "name": "data-forge-plot",
    "version": "0.2.5",
    "description": "Plotting API for use with Data-Forge.",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "scripts": {
        "c": "npm run clean",
        "clean": "rm -rf build/*",
        "b": "npm run build",
        "build": "tsc",
        "cb": "npm run clean-build",
        "clean-build": "npm run clean && npm run build",
        "build:watch": "tsc --watch",
        "clean-build:watch": "rm -rf build/* && tsc --watch",
        "prepublishOnly": "npm test && npm run clean-build",
        "l": "npm run lint",
        "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
        "pretest": "npm run lint",
        "t": "npm run test",
        "test": "jest",
        "tw": "npm run test:watch",
        "test:watch": "jest --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/data-forge/data-forge-plot.git"
    },
    "keywords": [],
    "author": "ashley@codecapers.com.au",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/data-forge/data-forge-plot/issues"
    },
    "homepage": "https://github.com/data-forge/data-forge-plot#readme",
    "dependencies": {
        "@data-forge-plot/apex": "0.0.7",
        "@data-forge-plot/chart-def": "^1.0.5",
        "@data-forge/serialization": "^1.0.0",
        "capture-template": "^1.1.10",
        "inflate-template": "^1.1.6",
        "opn": "^5.5.0",
        "typy": "^3.0.1"
    },
    "peerDependencies": {
        "data-forge": "^1.6.7"
    },
    "devDependencies": {
        "@types/jest": "^24.0.6",
        "@types/node": "^8.10.45",
        "jest": "^24.1.0",
        "ts-jest": "^23.10.5",
        "ts-node": "^8.0.3",
        "tslint": "^5.15.0",
        "typescript": "^3.4.3"
    }
}
